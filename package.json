{
  "name": "minecraft-randomizer-gui",
  "productName": "Minecraft Randomizer GUI",
  "version": "1.0.0",
  "description": "create random minecraft table data (loot_tables/recipes)",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "ciwin": "electron-builder --win",
    "cilinux": "electron-builder --linux",
    "cidarwin": "electron-builder --mac",
    "dist": "cross-env ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true electron-builder",
    "dist:win": "npm run dist -- --win --x64",
    "dist:mac": "npm run dist -- --mac",
    "dist:linux": "npm run dist -- --linux --x64"
  },
  "author": "Pharuxtan",
  "license": "MIT",
  "dependencies": {
    "child_process": "^1.0.2",
    "decompress-zip": "^0.2.2",
    "ejs-electron": "^2.0.3",
    "fs": "0.0.1-security",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "jarfile": "^2.0.0",
    "jquery": "^3.4.1",
    "ls-archive": "^1.3.1",
    "node-fetch": "^2.6.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "platform-folders": "^0.4.1",
    "random": "^2.1.1",
    "rekuire": "^0.1.9",
    "rimraf": "^3.0.0",
    "seedrandom": "^3.0.5",
    "temp-dir": "^2.0.0",
    "unzip": "^0.1.11",
    "url": "^0.11.0",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "electron": "^6.0.11",
    "electron-builder": "^21.2.0"
  },
  "build": {
    "appId": "com.minecraft.randomizer.gui"
  }
}
